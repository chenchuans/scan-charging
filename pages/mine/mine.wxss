/* pages/mine.wxss */
.login-now{
  box-sizing: border-box;
  width:720rpx;
  height: 100rpx;
  line-height: 80rpx;
  margin: 30rpx auto;
  /* background: rgba(164, 223, 110,0.6); */
  padding: 10rpx 30rpx;
}
.title {
  width: 750rpx;
  height: 50rpx;
  color: #4A4A4A;
  font: 24rpx/50rpx PingFangSC-Light;
  text-indent: 32rpx;
}
.link-contact{ margin: 30rpx;background:#f6f6f6;box-sizing:border-box;font-size:24rpx;color:#787878; padding: 0rpx 20rpx;line-height:72rpx;border:0 none;display:flex;justify-content: space-between;position:relative;align-items:center; font-family: PingFangSC-Regular;}
.link-go:after,.link-contact::before{content:"";display:block;width: 13rpx;height: 24rpx;position:absolute;right:30rpx;top:25rpx;background:url(data:image/png;base64,) no-repeat;background-size:cover;}
.link-contact::after{border:0;}
.assoc-us {
  position: absolute;
  width: 153rpx;
  height: 56rpx;
  right: 0;
  top: 20rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
}